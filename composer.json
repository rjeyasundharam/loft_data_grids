{
  "name": "aklump/loft_data_grids",
  "type": "library",
  "description": "PHP Classes to allow data placement in a grid structure and exporting in various data formats.",
  "keywords": [
    "spreadsheet",
    "csv",
    "excel",
    "export"
  ],
  "homepage": "http://www.intheloftstudios.com/packages/php/loft_data_grids",
  "authors": [
    {
      "name": "Aaron Klump",
      "email": "sourcecode@intheloftstudios.com",
      "homepage": "https://www.intheloftstudios.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1 || <=8.3",
    "symfony/yaml": "^5 || ^6",
    "phpoffice/phpspreadsheet": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "AKlump\\LoftDataGrids\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AKlump\\LoftDataGrids\\Tests\\Unit\\": "./tests_unit"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8 || ^9.6",
    "aklump/phpswap": "^0.0.8"
  },
  "license": "BSD-3-Clause"
}
